name = input('Enter name: ')
weight = int(input('Enter your weight in kg: '))
height = float(input('Enter your height in meters: '))
bmi = (weight / (height**2))
bmi = round(bmi, 1)

print('BMI =', bmi)

if bmi < 18.5:
 print(f'{name}, te encuentras bajo peso.')
elif bmi >= 18.5 and bmi <= 24.9:
 print(f'{name}, tu peso es normal.')
elif bmi >= 25.0 and bmi <= 29.9:
  print(f'{name}, estas con sobrepeso.')
else:
  print(f'{name}, usted se encuentra con obesidad.')
